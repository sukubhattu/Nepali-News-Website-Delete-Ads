{
  "name": "Image Ads Remover",
  "version": "1.0.0",
  "manifest_version": 2,
  "run_at": "document_idle",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["./js/content.js"]
    }
  ],
  "icons": { 
    "16": "./assets/icon.png",
    "48": "./assets/icon.png",
    "128": "./assets/icon.png" 
  }
}
