{
  "name": "Image Ads Remover",
  "description":"This extension removes image ads from Nepali news website",
  "version": "2.0.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["./js/content.js"]
    }
  ],
  "icons": { 
    "16": "./assets/icon.png",
    "48": "./assets/icon.png",
    "128": "./assets/icon.png" 
  },
  "permissions":[
    "storage"
  ]
}
